--Buying stuff
local Buy = Window:NewTab("Buying stuff")
local BuySection = Buy:NewSection("Zakupywacze")

BuySection:NewButton("Caik", "no zgadnij", function()
    local args = {
    [1] = "caik",
    [2] = false,
    [3] = true
}

game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Shop"):FireServer(unpack(args))
end)

